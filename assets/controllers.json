{
    "controllers": {
        "@survos/bootstrap-bundle": {
            "search": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": []
            }
        },
        "@survos/simple-datatables-bundle": {
            "table": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "@survos/simple-datatables/style.css": true
                }
            }
        },
        "@survos/tree-bundle": {
            "tree": {
                "enabled": true,
                "fetch": "lazy"
            },
            "api_tree": {
                "enabled": true,
                "fetch": "lazy"
            }
        },
        "@survos/workflow-helper-bundle": {
            "workflow": {
                "enabled": true,
                "fetch": "eager"
            }
        },
        "@symfony/ux-leaflet-map": {
            "map": {
                "enabled": true,
                "fetch": "lazy"
            }
        }
    },
    "entrypoints": []
}
