{
    "type": "project",
    "name": "survos-sites/pgsc",
    "description": "Popup Galleries of San Cristobal",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "api-platform/doctrine-orm": "^4.1",
        "api-platform/symfony": "^4.1",
        "doctrine/dbal": "^4.2.3",
        "doctrine/doctrine-bundle": "^2.13.2",
        "doctrine/doctrine-migrations-bundle": "^3.4.1",
        "doctrine/orm": "^3.3.2",
        "easycorp/easyadmin-bundle": "4.x-dev",
        "endroid/qr-code-bundle": ">=6",
        "inspector-apm/inspector-symfony": "^1.8.2",
        "league/csv": "^9.22",
        "nelmio/cors-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.6.1",
        "phpstan/phpdoc-parser": "^2.1",
        "survos/auth-bundle": "^1.5.529",
        "survos/bootstrap-bundle": "^1.5.529",
        "survos/command-bundle": "^1.5.529",
        "survos/deployment-bundle": "^1.5.529",
        "symfony/asset": "^7.2",
        "symfony/asset-mapper": "^7.2.3",
        "symfony/console": "^7.2.1",
        "symfony/doctrine-messenger": "^7.2.3",
        "symfony/dotenv": "^7.2",
        "symfony/expression-language": "^7.2",
        "symfony/flex": "^2.5",
        "symfony/form": "^7.2.4",
        "symfony/framework-bundle": "^7.2.4",
        "symfony/http-client": "^7.2.4",
        "symfony/intl": "^7.2",
        "symfony/mailer": "^7.2.3",
        "symfony/mime": "^7.2.4",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "^7.2",
        "symfony/process": "^7.2.4",
        "symfony/property-access": "^7.2.3",
        "symfony/property-info": "^7.2.3",
        "symfony/runtime": "^7.2.3",
        "symfony/security-bundle": "^7.2.3",
        "symfony/serializer": "^7.2.4",
        "symfony/stimulus-bundle": "^2.23",
        "symfony/string": "^7.2",
        "symfony/translation": "^7.2.4",
        "symfony/twig-bundle": "^7.2",
        "symfony/ux-icons": "^2.23",
        "symfony/ux-turbo": "^2.23",
        "symfony/validator": "^7.2.4",
        "symfony/web-link": "^7.2",
        "symfony/yaml": "^7.2.3",
        "symfonycasts/verify-email-bundle": "^1.17.3",
        "twig/extra-bundle": "^3.20",
        "twig/twig": "^3.20",
        "vich/uploader-bundle": ">=2.5.1"
    },
    "config": {
        "allow-plugins": {
            "endroid/installer": true,
            "php-http/discovery": true,
            "survos/installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "^7.2"
        }
    },
    "require-dev": {
        "bluemmb/faker-picsum-photos-provider": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "jzonta/faker-restaurant": "^2.0.1",
        "pelmered/fake-car": "^2.1",
        "phpunit/phpunit": "^9.6.22",
        "survos/maker-bundle": "^1.5.503",
        "symfony/browser-kit": "^7.2.4",
        "symfony/css-selector": "^7.2",
        "symfony/debug-bundle": "^7.2",
        "symfony/maker-bundle": "^1.62.1",
        "symfony/phpunit-bridge": "^7.2",
        "symfony/stopwatch": "^7.2.4",
        "symfony/web-profiler-bundle": "^7.2.4",
        "zenstruck/foundry": "^2.3.7"
    }
}
