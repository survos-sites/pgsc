{
    "type": "project",
    "name": "survos-sites/pgsc",
    "description": "Popup Galleries of San Cristobal",
    "license": "proprietary",
    "minimum-stability": "RC",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "alexandre-fernandez/json-translation-bundle": "^1.3.2",
        "api-platform/doctrine-orm": "^4.2.9",
        "api-platform/symfony": "^4.2.9",
        "doctrine/dbal": "^4.4.1",
        "doctrine/doctrine-bundle": "^2.18.1",
        "doctrine/doctrine-migrations-bundle": "^3.7.0",
        "doctrine/orm": "^3.5.8",
        "easycorp/easyadmin-bundle": "^4.27.5",
        "endroid/qr-code-bundle": ">=6.1",
        "fguillot/json-rpc": "^1.3.1",
        "genkgo/favicon": "^0.3.7",
        "google/apiclient": "^2.18.4",
        "inspector-apm/inspector-symfony": "^1.10.0",
        "league/csv": "^9.27.1",
        "nelmio/cors-bundle": "^2.6",
        "paragonie/sodium_compat": "^2.4",
        "phpdocumentor/reflection-docblock": "^5.6.5",
        "phpstan/phpdoc-parser": "^2.3",
        "stof/doctrine-extensions-bundle": "^1.15",
        "survos/auth-bundle": "^2.0.83",
        "survos/babel-bundle": "^2.0.83",
        "survos/bootstrap-bundle": "^2.0.83",
        "survos/command-bundle": "^2.0.83",
        "survos/core-bundle": "^2.0.83",
        "survos/deployment-bundle": "^2.0.83",
        "survos/flickr-bundle": "^2.0.83",
        "survos/geoapify-bundle": "^2.0.82",
        "survos/google-sheets-bundle": "^2.0.83",
        "survos/inspection-bundle": "^2.0.83",
        "survos/sais-bundle": "^2.0.83",
        "survos/simple-datatables-bundle": "^2.0.83",
        "survos/state-bundle": "^2.0.83",
      "symfony/asset": "^7.4 || ^8.0",
      "symfony/asset-mapper": "^7.4 || ^8.0",
      "symfony/console": "^7.4.1 || ^8.0",
      "symfony/doctrine-messenger": "^7.4.1 || ^8.0.1",
      "symfony/dotenv": "^7.4 || ^8.0",
      "symfony/expression-language": "^7.4 || ^8.0",
      "symfony/flex": "^2.10",
      "symfony/form": "^7.4.1 || ^8.0",
      "symfony/framework-bundle": "^7.4.1 || ^8.0",
      "symfony/http-client": "^7.4.1 || ^8.0",
      "symfony/intl": "^7.4 || ^8.0.1",
      "symfony/mailer": "^7.4 || ^8.0",
      "symfony/mime": "^7.4 || ^8.0",
      "symfony/monolog-bundle": "^3.11.1",
      "symfony/notifier": "^7.4 || ^8.0",
      "symfony/object-mapper": "^7.4 || ^8.0.1",
      "symfony/process": "^7.4 || ^8.0",
      "symfony/property-access": "^7.4 || ^8.0",
      "symfony/property-info": "^7.4.1 || ^8.0.1",
      "symfony/runtime": "^7.4.1 || ^8.0.1",
      "symfony/security-bundle": "^7.4 || ^8.0",
      "symfony/serializer": "^7.4.2 || ^8.0.2",
      "symfony/stimulus-bundle": "^2.31",
      "symfony/string": "^7.4 || ^8.0",
      "symfony/translation": "^7.4 || ^8.0",
      "symfony/twig-bundle": "^7.4 || ^8.0",
      "symfony/ux-icons": "^2.31",
      "symfony/ux-leaflet-map": "^2.31",
      "symfony/ux-map": "^2.31",
      "symfony/validator": "^7.4.2 || ^8.0.2",
      "symfony/web-link": "^7.4 || ^8.0",
      "symfony/workflow": "^7.4 || ^8.0",
      "symfony/yaml": "^7.4.1 || ^8.0.1",
        "symfonycasts/verify-email-bundle": "^1.18.0",
        "twig/extra-bundle": "^3.22.1",
        "twig/twig": "^3.22.1",
        "vich/uploader-bundle": ">=2.9.1"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "endroid/installer": true,
            "php-http/discovery": true,
            "survos/installer": false,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "geocode": "bin/console workflow:iterate App\\\\Entity\\\\Location --transition=geocode --marking=new -vv\n"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "endpoint": ["https://api.github.com/repos/agence-adeliom/symfony-recipes/contents/index.json?ref=flex/main", "flex://defaults"],
            "allow-contrib": true
        }
    },
   "require-dev": {
        "bluemmb/faker-picsum-photos-provider": "^2.0",
        "dbrekelmans/bdi": "^1.4.1",
        "doctrine/doctrine-fixtures-bundle": "^4.3.1",
        "jzonta/faker-restaurant": "^2.0.1",
        "pelmered/fake-car": "^2.1",
        "phpstan/phpstan": "^2.1.33",
        "phpunit/phpunit": "^12.5.3",
        "survos/code-bundle": "dev-main",
        "survos/crawler-bundle": ">=2.0.83",
        "survos/doc-bundle": "^1.6.44",
        "symfony/browser-kit": "^7.4.0",
        "symfony/css-selector": "^7.4",
        "symfony/debug-bundle": "^7.4",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/panther": "^2.3",
        "symfony/stopwatch": "^8.0",
        "symfony/web-profiler-bundle": "^8.0",
        "zenstruck/browser": "^1.9.1",
        "zenstruck/console-test": "^1.8",
        "zenstruck/foundry": "^2.8.2"
    },
    "repositories": {
        "behaviors": {
            "type": "vcs",
            "url": "git@github.com:tacman/DoctrineBehaviors.git"
        }
    }
}
